<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hire.pojo.SeekerPojo" table="seeker">
		<id name="seeker_id">
			<generator class="increment"></generator>
		</id>
		<property name="seeker_name"></property>
		<property name="seeker_email"></property>
		<property name="seeker_mobile"></property>
		<property name="seeker_password"></property>
		<property name="seeker_resume"></property>
		<property name="seeker_experience"></property>
		<property name="seeker_cur_company"></property>
		<property name="seeker_cur_salary"></property>
	</class>
</hibernate-mapping>